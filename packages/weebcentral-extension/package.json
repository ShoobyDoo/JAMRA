{
  "name": "@jamra/weebcentral-extension",
  "version": "1.0.0",
  "type": "module",
  "description": "WeebCentral manga source extension for JAMRA",
  "main": "dist/bundle.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node build.mjs && tsc --emitDeclarationOnly && pnpm run postbuild",
    "postbuild": "mkdir -p ../../.jamra-data/extensions && cp dist/bundle.cjs ../../.jamra-data/extensions/com_weebcentral_manga@1.0.0.cjs",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "keywords": [
    "manga",
    "weebcentral",
    "mangasee",
    "extension"
  ],
  "author": "JAMRA",
  "license": "MIT",
  "dependencies": {
    "@jamra/extension-sdk": "workspace:*",
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.10",
    "typescript": "^5.3.3"
  }
}
